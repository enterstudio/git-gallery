<div class="row">
  <div class="span12">
    <h1>Gallery Members</h1>

    <ul class="gallery-container">  
      <% @users.each do |user| %>
        <li>
          <%= link_to (image_tag avatar_url(user)), user %><br>
          <%= link_to "#{user.name}", user %>
        </li>
      <% end %>
    </ul>
  </div>
</div>