<div class="inline-custom">
  <div class="container user_show">
    <div class="row-fluid span12">
      <div class="span8 float-custom-left">
        <div class="bs-docs-example">
          <div class="section-wrapper clearfix">

            <h3 class="section-header">Projects</h3>

            <% @projects.each_with_index do |project, index| %>
              <%= render :partial => "projects/partials/index_card", :locals => { :project => project, :index => index } %>
            <% end %>

          </div>

        </div>
      </div>
      <div class="span3 float-custom-right">

        <div class="blog-categories widget-tp">
          <h3>Project Preview</h3>
          <div class="project-full-description">
            <% @projects.each_with_index do |project, index| %>
              <%= render :partial => "projects/partials/project_full_description", :locals => { :project => project, :index => index } %>
            <% end %>
          </div>
        </div>


      </div>
    </div>
  </div>
</div>

