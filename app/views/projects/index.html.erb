

<div class="inline-custom"> 
<div class="container user_show">
  <div class="row-fluid">
      <div class="span12 float-custom-left">
        <div class="bs-docs-example">

<div class="section-wrapper">
  <h3 class="section-header">Projects</h3>

<div class="row project-row-custom">
<% @projects.each do |project| %>

  <div class="span4">
    <div class="white-card recent-post project-post-custom">
      <h5 class="recent-post-header">
        <a href="<%= project_path(project)%>"><%= project.name %></a>
      </h5>
      <div class="post-info clearfix">
  
      </div>
    <%= project_image(project) %>
  
  <p class="post-content separated"><%= project.description %></p>
  
  <div class="technologies-custom">
    <% project.technologies.each do |tech| %>
    <span class="label"><%=tech.name%></span>
          
          <% end %>
  </div>
  </div>

  </div>


<% end %>
  </div>
  </div>
        </div>
      </div>
  


       </div>
  
</div>
</div>


