<p id="notice"><%= notice %></p>

<p>
  <b>Title:</b>
  <%= @project.title %>
</p>

<p>
  <b>Users:</b>
  <%= @project.user.name %>
</p>

<p>
  <b>Technologies:</b>
  <ul>
  <% @project.technologies.each do |technology| %>
    <li><%= technology.name %></li>
  <% end %>
  </ul>
</p>

<p>
  <b>Description:</b>
  <%= @project.description %>
</p>


<%= link_to 'Edit', edit_user_project_path(@project.user, @project) %> |
<%= link_to 'Back', user_projects_path %>
