<div id="contributors">
  <h3>Contributors</h3>
  <ul class="gg-list-inline">
    <% @project.users.uniq.each do |user| %>
      <li><div class="gg-list-inline">
        <%= image_tag avatar_url(user) %><br>
        <%= link_to user.name, user_path(user) %></div>
      </li>
    <% end %>
  </ul>
</div>